<h2>Your videos (<%= @videos.count%>)</h2>
  <div class="grid">
    <% @videos.each do |video| %>
      <%= link_to video_path(video) do %>
        <div class="grid-item card">
          <%# cl_image_tag video.photo.key, :height=>200, :crop=>"fill", :class => "img-card" %>
          <div class="card-body">
            <p> <%= video.description %></p>
            <h5> <%= video.title %></h5>
            <p> <%= video.created_at %></p>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>